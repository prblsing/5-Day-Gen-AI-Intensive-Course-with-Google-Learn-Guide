   "cell_type": "code",
   "execution_count": 4,
   "id": "d3f1a91b",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-04-05T02:24:58.837426Z",
     "iopub.status.busy": "2025-04-05T02:24:58.836776Z",
     "iopub.status.idle": "2025-04-05T02:24:59.034454Z",
     "shell.execute_reply": "2025-04-05T02:24:59.033307Z"
    },
    "papermill": {
     "duration": 0.214504,
     "end_time": "2025-04-05T02:24:59.036990",
     "exception": false,
     "start_time": "2025-04-05T02:24:58.822486",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "from google.api_core import retry\n",
    "\n",
    "\n",
    "is_retriable = lambda e: (isinstance(e, genai.errors.APIError) and e.code in {429, 503})\n",
    "\n",
    "genai.models.Models.generate_content = retry.Retry(\n",
    "    predicate=is_retriable)(genai.models.Models.generate_content)"
   ]
  },
